function center_bottom_slide(){bottom_slide_inner.each(function(){bottom_slide_inner.css('left',(window_width-bottom_slide_width)/2);});}
function animate_bottom_slide(e,pos_len){var left_pos=0;if(window_width>bottom_slide_width){left_pos=(window_width-bottom_slide_width)/2;}else if(e==null){left_pos=(window_width-bottom_slide_width)*pos_len;}else{left_pos=(window_width-bottom_slide_width)*(get_mouse_smooth(e.pageX)/window_width);}
bottom_slide_inner.animate({left:left_pos},{queue:false,duration:400,easing:'easeOutExpo'});}
function get_mouse_smooth(mouse_pos){if(mouse_pos<200){return mouse_pos/10;}else if(window_width-mouse_pos<200){return window_width-(window_width-mouse_pos)/10;}
return mouse_pos;}
